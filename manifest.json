{
    
    "name": "Search-AI(uses ChatGPT)",
    "description": "A small open AI tool used to streamline all your chatgpt searches at one place",
    "author": "Parth Bhatnagar",
    "version": "0.0.1",
    "manifest_version": 3,
   "icons": 
       {
    "96": "firdos.png"
   },
  
    
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "runAt": "document_end",
            "js": ["script.js"],
            "all_frames": true
        }
    ]
}
