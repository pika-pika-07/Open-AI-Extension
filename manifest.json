{
    
    "name": "Firdos AI(uses ChatGPT)",
    "description": "Using openAI as chrome extension",
    "author": "Parth Bhatnagar",
    "version": "0.0.1",
    "manifest_version": 3,
   "icons": 
       {
    "96": "firdos.png"
   },
   "permissions": ["storage", "activeTab"],
    
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "runAt": "document_end",
            "js": ["script.js"],
            "all_frames": true
        }
    ]
}
